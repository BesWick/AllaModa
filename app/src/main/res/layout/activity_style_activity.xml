<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".style_activity">

    <Button
            android:background="#891018"
            android:textColor="#FFFFFF" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.946"
            android:text="BACK TO HOME PAGE"
            android:layout_width="159dp"
            android:layout_height="100dp"
            android:fontFamily="serif-monospace"
            android:id="@+id/button16" app:layout_constraintHorizontal_bias="0.093"/>
    <Button
            android:background="#891018"
            android:textColor="#FFFFFF" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.946"
            android:text="Select"
            android:layout_width="159dp"
            android:layout_height="100dp"
            android:fontFamily="serif-monospace"
            android:id="@+id/button17" app:layout_constraintHorizontal_bias="0.966"/>
</android.support.constraint.ConstraintLayout>