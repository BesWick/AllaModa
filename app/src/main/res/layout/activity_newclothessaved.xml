<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".newclothessaved">

    <TextView
            android:text="Your Clothes Are Saved!"
            android:layout_width="379dp"
            android:layout_height="71dp"
            android:textSize="35dp"
            android:id="@+id/textView2"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="0.474" app:layout_constraintVertical_bias="0.032"/>

    <TextView
            android:text="What would you like to do now?"
            android:layout_width="292dp"
            android:layout_height="66dp"
            android:textSize="20dp"
            android:id="@+id/textView3"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="0.459" app:layout_constraintVertical_bias="0.118"/>

    <Button
            android:text="Create An Outfit"
            android:textColor="#FFFFFF"
            android:background="#891018"
            android:layout_width="350dp"
            android:layout_height="72dp"
            android:id="@+id/button13" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <Button
            android:text="Add More Clothes"
            android:textColor="#FFFFFF"
            android:background="#891018"
            android:layout_width="350dp"
            android:layout_height="72dp"
            android:id="@+id/button14" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.511"
            app:layout_constraintVertical_bias="0.673"/>
    <Button
            android:text="Back to Home Page"
            android:textColor="#FFFFFF"
            android:background="#891018"
            android:layout_width="350dp"
            android:layout_height="72dp"
            android:id="@+id/button15" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.511"
            app:layout_constraintVertical_bias="0.847"/>
</android.support.constraint.ConstraintLayout>